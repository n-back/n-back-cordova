<?xml version='1.0' encoding='utf-8'?>
<widget id="org.nback" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>N-back</name>
    <description>
        N-back Game
    </description>
    <author email="windworst@gmail.com" href="https://n-back.github.io">
        Xbw
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-hot-code-push-plugin" spec="^1.5.3" />
    <chcp>
        <config-file url="http://n-back.github.io/chcp.json" />
    </chcp>
    <preference name="ShowSplashScreenSpinner" value="false" />
    <engine name="android" spec="^7.1.0" />
    <engine name="ios" spec="^4.5.4" />
</widget>
